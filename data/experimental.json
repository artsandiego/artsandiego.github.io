{
  "type": "FRAME",
  "name": "My Frame",
  "width": 300,
  "height": 400,
  "layoutMode": "VERTICAL",
  "primaryAxisSizingMode": "FIXED",
  "itemSpacing": 8,
  "counterAxisSizingMode": "AUTO",
  "primaryAxisAlignItems": "MIN",
  "counterAxisAlignItems": "MIN",
  "clipsContent": true,
  "children": [
    {
      "type": "RECTANGLE",
      "name": "My Rectangle",
      "width": 100,
      "height": 100,
      "fills": [
        {
          "type": "SOLID",
          "color": {
            "r": 1,
            "g": 0.5,
            "b": 0,
            "a": 1
          }
        }
      ]
    },
    {
      "type": "TEXT",
      "name": "My Text",
      "characters": "Hello, world!",
      "fontName": {
        "family": "Roboto",
        "style": "Regular"
      },
      "fontSize": 24,
      "textAlignHorizontal": "CENTER",
      "textAlignVertical": "CENTER"
    }
  ]
}